#include <stdio.h>

int main() {
    printf("NAME : Manya Dk\nUSN : 1SV24CS073\n");

    int arr[4] = {10, 20, 30, 40};
    int n = 4, i;
    for (i = 0; i < n - 1; i++) {
        arr[i] = arr[i + 1];
    }

    n--; 
    for (i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
