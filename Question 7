#include <stdio.h>
#include <stdlib.h>

int* mergeSorted(int *a, int n, int *b, int m, int *size) {
    int *res = malloc((n + m) * sizeof(int)), i=0,j=0,k=0;
    while (i<n && j<m) {
        if (a[i]<b[j]) res[k++] = a[i++];
        else if (b[j]<a[i]) res[k++] = b[j++];
        else { res[k++] = a[i++]; j++; }
    }
    while (i<n) res[k++] = a[i++];
    while (j<m) res[k++] = b[j++];
    res = realloc(res, k * sizeof(int));
    *size = k;
    return res;
}
